<h1 id="individualmuscleexercises">Individual Muscle Exercises</h1>

	<ul id="muscle">
	    <li id="arms"><%= link_to 'Arms', arm_path, { onmouseover: "handleMouseOver('.arms')", onmouseout: "handleMouseOut('.arms')" } %></li><br>

	    <li id="shoulders"><%= link_to 'Shoulders', shoulder_path, { onmouseover: "handleMouseOver('.shoulders')", onmouseout: "handleMouseOut('.shoulders')" } %></li><br>

	    <li id="chest"><%= link_to 'Chest', chest_path, { onmouseover: "handleMouseOver('.chest')", onmouseout: "handleMouseOut('.chest')" } %></li><br>

	    <li id="abs"><%= link_to 'Abs', abs_path, { onmouseover: "handleMouseOver('.abs')", onmouseout: "handleMouseOut('.abs')" } %></li><br>

	    <li id="back"><%= link_to 'Back', back_path, { onmouseover: "handleMouseOver('.back')", onmouseout: "handleMouseOut('.back')" } %></li><br>

	    <li id="legs"><%= link_to 'Legs', leg_path, { onmouseover: "handleMouseOver('.legs')", onmouseout: "handleMouseOut('.legs')" } %></li><br>

	    <li id="cardio"><%= link_to 'Cardio', cardio_path, { onmouseover: "handleMouseOver('.cardio')", onmouseout: "handleMouseOut('.cardio')" } %></li><br>
	</ul>


	<div id="muscleman">
		<%= image_tag image_path("musclegroups.png"), class: "man" %>
	</div>


	<div id="touchpoints">
		<i id="shoulders1" class="fa fa-circle fa-lg shoulders" aria-hidden="true"></i>
